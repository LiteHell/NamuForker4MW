{
  "name": "namuforker4mw",
  "version": "2.0.1",
  "engines": {
    "node": ">=4.2.6"
  },
  "description": "forks a article from namuwiki (https://namu.wiki) and saves it in Mediawiki exported xml format.",
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/LiteHell/NamuForker4MW/issues",
    "email": "LiteHell@openmailbox.org"
  },
  "keywords": [
    "wiki",
    "namu",
    "namu.wiki",
    "fork",
    "mediawiki",
    "forker"
  ],
  "preferGlobal": true,
  "author": {
    "name": "LiteHell",
    "email": "LiteHell@openmailbox.org"
  },
  "files": [
    "*.js",
    "forker",
    "forker/*.js",
    "package.json",
    "READMD.md"
  ],
  "bin": {
    "namuforker": "./index.js"
  },
  "license": "MIT",
  "repository": "https://github.com/LiteHell/NamuForker4MW.git",
  "dependencies": {
    "chalk": "^1.1.1",
    "cheerio": "^0.19.0",
    "cloudscraper": "git+https://github.com/LiteHell/cloudscraper.git",
    "extend": "^3.0.0",
    "request": "^2.67.0",
    "xmlbuilder": "^4.2.0"
  },
  "devDependencies": {
    "request-debug": "^0.2.0"
  }
}
